{
    "name": "civicrm/strings",
    "description": "String extractor for CiviCRM",
    "license": "GPL-2+",
    "require": {
        "php": ">=7.4.0",
        "symfony/console": "~5.4",
        "nikic/php-parser": "~4.13",
        "rubobaquero/phpquery": "^0.9.15"
    },
    "authors": [
        {
            "name": "CiviCRM",
            "email": "info@civicrm.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Civi\\Strings\\": "src/",
            "Civi\\": "extern/Civi/"
         },
         "psr-0": {
           "CRM_": "extern/"
         }
    },
    "bin": [
        "bin/civistrings"
    ],
    "config": {
        "platform": {
            "php": "7.4.0"
        },
        "bin-dir": "bin"
    },
    "require-dev": {
        "phpunit/phpunit": "9.6.*"
    }
}
